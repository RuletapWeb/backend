    "name": {
      "type": "string"
    },
    "phone": {
      "required": true,
      "unique": true,
      "type": "biginteger"
    },
    "lastPlayed": {
      "type": "datetime",
      "default": "2021-01-01T15:00:00.000Z"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    }
  }
}
