{
  "kind": "collectionType",
  "collectionName": "players",
  "info": {
    "name": "player",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
